# Copy this file to `.env` and fill in the required values.

# === Core Settings ===
APP_ENV=development
APP_LOG_LEVEL=INFO
APP_DATA__DATA_DIR=./storage

# === LLM / Embeddings ===
APP_OPENAI_API_KEY=changeme
APP_LLM__COMPLETION_MODEL=gpt-4o-mini
APP_LLM__EMBEDDING_MODEL=text-embedding-3-large

# === Security ===
APP_SECURITY__JWT_SECRET=change-this-secret
APP_SECURITY__HMAC_SHARED_SECRET=change-this-hmac-secret

# === RBAC ===
APP_DEFAULT_ADMIN_EMAIL=admin@example.com
APP_DEFAULT_ADMIN_PASSWORD=please-change

# === Observability ===
APP_OBSERVABILITY__OTLP_ENDPOINT=http://localhost:4317

